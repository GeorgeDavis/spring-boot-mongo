{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Status Messages Schema",
  "type": "object",
  "properties": {
    "code":{
      "description": "Code",
      "type": "string"
    },
    "subCode":{
      "description": "SubCode",
      "type": "string"
    },
    "type":{
      "description": "Type",
      "type": "string",
      "enum": [
        "INFORMATION",
        "WARNING",
        "ERROR"
      ]
    },
    "message":{
      "description": "Message",
      "type": "string"
    }
  }
}